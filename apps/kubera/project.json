{
	"name": "kubera",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/kubera",
	"projectType": "application",
	"tags": [],
	"// targets": "to see all targets run: nx show project kubera --web",
	"targets": {
		"build": {
			"executor": "@nx/next:build",
			"outputs": ["{options.outputPath}"],
			"defaultConfiguration": "production",
			"options": {
				"outputPath": "dist/apps/kubera"
			},
			"configurations": {
				"development": {
					"outputPath": "apps/kubera"
				},
				"production": {}
			}
		},
		"serve": {
			"executor": "@nx/next:server",
			"defaultConfiguration": "development",
			"options": {
				"buildTarget": "kubera:build",
				"dev": true,
				"port": 2004
			},
			"configurations": {
				"development": {
					"buildTarget": "kubera:build:development",
					"dev": true
				},
				"production": {
					"buildTarget": "kubera:build:production",
					"dev": false
				}
			}
		},
		"export": {
			"executor": "@nx/next:export",
			"options": {
				"buildTarget": "kubera:build:production"
			}
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "apps/kubera/jest.config.ts"
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint"
		},
		"analyze": {
			"executor": "@nx/next:build",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/apps/kubera"
			},
			"configurations": {
				"production": {
					"outputPath": "dist/apps/kubera",
					"env": {
						"ANALYZE": "true"
					}
				}
			}
		},
		"prisma-db-pull": {
			"executor": "nx:run-commands",
			"options": {
				"command": "npx prisma db pull --schema=db/prisma/schema.prisma"
			}
		},
		"prisma-db-push": {
			"executor": "nx:run-commands",
			"options": {
				"command": "npx prisma db push --schema=db/prisma/schema.prisma"
			}
		},
		"prisma-generate": {
			"executor": "nx:run-commands",
			"options": {
				"command": "npx prisma generate --schema=db/prisma/schema.prisma"
			}
		},
		"prisma-migrate-dev": {
			"executor": "nx:run-commands",
			"options": {
				"command": "npx prisma migrate dev --schema=db/prisma/schema.prisma"
			}
		},
		"prisma-migrate-deploy": {
			"executor": "nx:run-commands",
			"options": {
				"command": "npx prisma migrate deploy --schema=db/prisma/schema.prisma"
			}
		},
		"prisma-studio": {
			"executor": "nx:run-commands",
			"options": {
				"command": "npx prisma studio --schema=db/prisma/schema.prisma"
			}
		}
	}
}
